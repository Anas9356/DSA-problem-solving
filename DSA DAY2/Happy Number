class Solution:
    def isHappy(self, n: int) -> bool:
        s1=n
        f1=self.sumD(n)
        while f1!=1 and f1!=s1:
            s1=self.sumD(s1)
            f1=self.sumD(self.sumD(f1))
        return f1==1;    

        

    def sumD(self, num: int) -> int:
        sum=0
        while num>0:
            dig=num%10
            sum+=dig*dig
            num=num//10
        return sum
