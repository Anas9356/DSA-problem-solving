class Solution:
    def maxArea(self, height: List[int]) -> int:
        p1=0
        p2=len(height)-1
        cap=-1
        while p1<p2:
            w=p2-p1
            h=min(height[p1],height[p2])
            area=h*w
            if area >cap:
                cap=area
            if height[p1]>=height[p2]:
                p2-=1
            else:
                p1+=1

        return cap



        
